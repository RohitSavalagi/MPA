{
  "name": "@netcracker/boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "hmr": "ng serve --hmr",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "serve-dist": "lite-server --baseDir=\"dist/boilerplate/\"",
    "test": "ng test",
    "svg": "gulp svg -f ./config/gulpfile.js",
    "e2e": "ng e2e",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx,.js,.jsx --fix",
    "stylelint": "stylelint \"src/**/*.less\" --custom-syntax postcss-less",
    "stylelint:fix": "stylelint --fix \"src/**/*.less\" --custom-syntax postcss-less",
    "report": "webpack-bundle-analyzer dist/boilerplate/stats.json",
    "precommit": "lint-staged --no-stash"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.3",
